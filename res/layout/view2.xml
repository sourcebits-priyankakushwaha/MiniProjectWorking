<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:ignore="TextFields" >

    <TextView
        android:id="@+id/view2_tv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name" />

    <TextView
        android:id="@+id/view2_tv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@+id/view2_tv1" />

    <TextView
        android:id="@+id/view2_tv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view2_tv1"
        android:text="ID" />

    <TextView
        android:id="@+id/view2_tv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@+id/view2_tv3" />

    <TextView
        android:id="@+id/view2_tv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view2_tv4"
        android:text="MATH MARKS" />

    <TextView
        android:id="@+id/view2_tv6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@+id/view2_tv5" />

    <TextView
        android:id="@+id/view2_tv7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view2_tv5"
        android:text="ENGLISH MARKS" />

    <TextView
        android:id="@+id/view2_tv8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@+id/view2_tv7" />

    <TextView
        android:id="@+id/view2_tv9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view2_tv7"
        android:text="SCIENCE MARKS" />

    <TextView
        android:id="@+id/view2_tv10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@+id/view2_tv9" />

    <TextView
        android:id="@+id/view2_tv11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view2_tv9"
        android:text="HISTORY MARKS" />

    <TextView
        android:id="@+id/view2_tv12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@+id/view2_tv11" />

    <TextView
        android:id="@+id/view2_tv13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view2_tv11"
        android:text="SOCIALSCIENCE MARKS" />

    <TextView
        android:id="@+id/view2_tv14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@+id/view2_tv13" />

</LinearLayout>